# LiveKit credentials (для валидации подписи webhook)
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=devsecret

# URL ноды блокчейна (chain API)
BLOCKCHAIN_API_URL=https://api-testnet.coopenomics.world

# Имя контракта registrator (по умолчанию: registrator)
REGISTRATOR_CONTRACT=registrator

# Интервал обновления реестра кооперативов (мс, по умолчанию: 300000 = 5 минут)
COOP_REFRESH_INTERVAL_MS=300000

# Таймаут при пересылке webhook на контроллер (мс, по умолчанию: 5000)
FAN_OUT_TIMEOUT_MS=5000

# Порт HTTP-сервера (по умолчанию: 3100)
PORT=3100
